.row_border_single_dark{ border-bottom:1px solid $darkBorder; }
.box_border{ border:1px solid $lightBorder; }

.black_top{ border-top:3px solid $darkBorder; }
.black_bottom{ border-bottom:3px solid $darkBorder; }

/*forms*/

input.right{
    width:auto;
    float:right;
}

.iPhoneCheckContainer{
    position:absolute;
    right:0px;
    top:-3px;
}

/*END MODULAR*/

.iPhoneCheckContainer{
    position:relative;
    width:85px;
    height:27px;
    cursor:pointer;
    overflow:hidden;
}
.iPhoneCheckContainer input{
    position:absolute;
    top:5px;
    left:30px;
}
.iPhoneCheckHandle{ 
    display:block;
    height:27px;
    width:39px;
    cursor:pointer;
    position:absolute;
    top:0;
    left:0;
}
.iPhoneCheckHandle .iPhoneCheckHandleBG{
    position:absolute;
    width:5px;
    height:100%;
    top:0;
    left:0;
    z-index:1;
}
.iPhoneCheckHandle .iPhoneCheckHandleSlider{
    position:absolute;
    top:0;
    left:0;
    height:27px;
    width:39px;
    background:url(../../images/iphone-style-checkboxes/iphone-slider.png) no-repeat;
    z-index:2;
}
label.iPhoneCheckLabelOn,label.iPhoneCheckLabelOff{
    font-size:17px;
    line-height:17px;
    font-weight:bold;
    font-family:Helvetica Neue,Arial,Helvetica,sans-serif;
    text-transform:uppercase;
    cursor:pointer;
    display:block;
    height:22px;
    position:absolute;
    width:52px;
    top:0;
}
label.iPhoneCheckLabelOn{
    color:#fff;
    background:url(../../images/iphone-style-checkboxes/iphone-on.png) no-repeat;
    text-shadow:0px 0px 2px rgba(0,0,0,0.6);
    left:0;
    padding:5px 0 0 8px;
}
label.iPhoneCheckLabelOff{
    color:#8B8B8B;
    background:url(../../images/iphone-style-checkboxes/iphone-off.png) no-repeat right 0;
    text-shadow:0px 0px 2px rgba(255,255,255,0.6);
    text-align:right;
    right:0;
    padding:5px 8px 0 0;
}

.big_button{
    padding:6px 15px 6px 15px;
    font-size:$fontLgSize;
}

.small_button{
    padding:5px 5px 4px 5px;
    font-size:$fontDefaultSize;
}
.icon{
    height:22px;
    width:23px;
    padding-left:25px;
    padding-top:5px;
    position:relative;
    top:0px;
    display:inline;
}

.icon.comment{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -81px;
}

.icon.repost{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -172px;
}

.icon.like{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -539px;
}

.icon.convo{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -1047px;
}

.icon.camera{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -1003px;
}

.icon.share{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -1097px;
}

.icon.delete{
    background:{
        image:url(../../images/icons_small-v8.png);
        repeat:no-repeat;
        position:5px -409px;
    }
}

.icon.edit{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -625px;
}

.icon.move{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -1135px;
}

.icon.voteup{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -816px;
    height:18px;
    top:-5px;
}
.icon.voteup.selected{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -868px;
    height:18px;
    top:-5px;
}
.icon.voteup:hover{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -868px;
}

.icon.pointup{
    background-image:url(../../images/icons_small-v8.png);
    background-repeat:no-repeat;
    background-position:0px -1220px;
}

