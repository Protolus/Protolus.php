    .close_button{
        width:18px;
        height:18px;
        color:$contrastText;
        border:1px solid $toolbarBg;
        border-radius:9px;
        -moz-border-radius:9px;
        -webkit-border-radius:9px;
        background: rgb(226,226,226);
        background: -moz-linear-gradient(top, rgba(red($toolbarBg),green($toolbarBg),blue($toolbarBg),1) 0%, rgba(red($toolbarBg),green($toolbarBg),blue($toolbarBg),1) 0%, rgba(red($darkBg),green($darkBg),blue($darkBg),1) 50%, rgba(red($darkBg),green($darkBg),blue($darkBg),1) 51%);
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(red($toolbarBg),green($toolbarBg),blue($toolbarBg),1)), color-stop(0%,rgba(red($toolbarBg),green($toolbarBg),blue($toolbarBg),1)), color-stop(50%,rgba(red($darkBg),green($darkBg),blue($darkBg),1)), color-stop(51%,rgba(red($darkBg),green($darkBg),blue($darkBg),1)));
        background: -webkit-linear-gradient(top, rgba(red($toolbarBg),green($toolbarBg),blue($toolbarBg),1) 0%,rgba(red($toolbarBg),green($toolbarBg),blue($toolbarBg),1) 0%,rgba(red($darkBg),green($darkBg),blue($darkBg),1) 50%,rgba(red($darkBg),green($darkBg),blue($darkBg),1) 51%);
        background: linear-gradient(top, rgba(red($toolbarBg),green($toolbarBg),blue($toolbarBg),1) 0%,rgba(red($toolbarBg),green($toolbarBg),blue($toolbarBg),1) 0%,rgba(red($toolbarBg),green($toolbarBg),blue($toolbarBg),1) 50%,rgba(red($toolbarBg),green($toolbarBg),blue($toolbarBg),1) 51%);
        position:absolute;
        right:-14px;
        top:-16px;
        z-index:500;
        text-align:center;
        font-size:14px;
        font-weight:bold;
        margin-bottom:20px;
        *{
            position:absolute;
            top:3px;
            left:5px;
        }
    }
    .close_button:hover{
        border:1px solid $contrastText;
    }
}
