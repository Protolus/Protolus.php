    text-align:center;
    -moz-border-radius:5px;
    width:300px;
    min-height : 50px;
    color:$contrastText;
    -webkit-box-shadow: 0px 0px 8px 2px rgba(red($darkBg),green($darkBg),blue($darkBg),0.7);
    -moz-box-shadow: 0px 0px 8px 2px rgba(red($darkBg),green($darkBg),blue($darkBg),0.7);
    box-shadow: 0px 0px 8px 2px rgba(red($darkBg),green($darkBg),blue($darkBg),0.7);
    .dialogue_box_inner{
        background:rgba(red($darkBg),green($darkBg),blue($darkBg),1);
