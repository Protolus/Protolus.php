/* FEED STYLES (XFEED) */
.feed_display{
    background-color: $canvas;
    padding:{
        top:45px;
        bottom:20px;
        right:20px;
        left:20px;
    }
    font-family:Arial;
    font-size: 14px;
    ol.paginator{
        width : 100%;
        display : none;
        text-align :center;
        li{
            margin-right: 5px;
            display : inline-block;
            padding :5px;
            text-shadow: 3px 3px 0 #000,
                -1px -1px 0 #000,
                1px -1px 0 #000,
                -1px 1px 0 #000,
                1px 1px 0 #000;
            -moz-text-shadow: $shadow 3px 3px 3px;
            font-weight: bold;
            color:$highlight;
            font-size: 20px;
            background-color: #550000;
            -moz-border-radius: 20px 20px 20px 20px;
            border-radius: 20px 20px 20px 20px;
            width: 20px;
            bottom: -5px;
            position:relative;
            background:url(../../images/header_background_x.jpg) repeat center;
            border:1px solid $shadow;
            -webkit-box-shadow:inset 3px 3px 10px $shadow;
            -moz-box-shadow:inset 3px 3px 10px $shadow;
            box-shadow:inset 3px 3px 10px $shadow;
        }
        li a:hover, li:hover{
            color:$lightText;
        }
        .next:before{
            content:'\2192';
        }
        .last:before{
            content:'\21E5';
        }
        .previous:before{
            content:'\2190';
        }
        .first:before{
            content:'\21E4';
        }
        .next, .last, .previous, .first{
            overflow:hidden;
            opacity:1;
            position:relative;/* hack due to slideshow styles */
            a{
                opacity:.001;
                margin-left:-20px;
            }
        }
    }

}
