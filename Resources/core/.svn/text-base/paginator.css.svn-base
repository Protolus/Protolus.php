/* SIZING (XSIZING) */
ol.paginator {
  width: 100%;
  display: none;
  text-align: center; }
  ol.paginator li {
    margin-right: 5px;
    display: inline-block;
    padding: 5px;
    text-shadow: 3px 3px 0 black, -1px -1px 0 black, 1px -1px 0 black, -1px 1px 0 black, 1px 1px 0 black;
    -moz-text-shadow: black 3px 3px 3px;
    font-weight: bold;
    color: #16a6b7;
    font-size: 20px;
    background-color: #550000;
    -moz-border-radius: 20px 20px 20px 20px;
    border-radius: 20px 20px 20px 20px;
    width: 20px;
    bottom: -5px;
    position: relative;
    background: url(../images/header_background_x.jpg) repeat center;
    border: 1px solid black;
    -webkit-box-shadow: inset 3px 3px 10px black;
    -moz-box-shadow: inset 3px 3px 10px black;
    box-shadow: inset 3px 3px 10px black; }
  ol.paginator li a:hover {
    color: #b6b6b6; }
  ol.paginator li:hover {
    color: #b6b6b6; }
  ol.paginator .next:before {
    content: '\2192'; }
  ol.paginator .last:before {
    content: '\21E5'; }
  ol.paginator .previous:before {
    content: '\2190'; }
  ol.paginator .first:before {
    content: '\21E4'; }
  ol.paginator .next, ol.paginator .last, ol.paginator .previous, ol.paginator .first {
    overflow: hidden;
    opacity: 1;
    position: relative;
    /* hack due to slideshow styles */ }
    ol.paginator .next a, ol.paginator .last a, ol.paginator .previous a, ol.paginator .first a {
      opacity: .001;
      margin-left: -20px; }
