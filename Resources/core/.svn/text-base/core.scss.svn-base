        -webkit-box-shadow:inset 15px 0px 15px 2px rgba(red($darkBg),green($darkBg),blue($darkBg),0.3);
        -moz-box-shadow:inset 15px 0px 15px 2px rgba(red($darkBg),green($darkBg),blue($darkBg),0.3);
