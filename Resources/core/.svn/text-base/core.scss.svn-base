    .share_icon.blogger.off{ background-position:0px -650px; }
    
.dark_link, .dark_link:visited, .dark_link:link{
    color:$text;
    .light_link:hover{
        color:$text;
        text-decoration:underline;
    }
}    

.light_link, .light_link:visited, .light_link:link{
    color:$text;
    .light_link:hover{
        color:$text;
        text-decoration:underline;
    }
}

.contrast_link, .contrast_link:visited, .contrast_link:link{
    color:$contrastText;
    .light_link:hover{
        color:$contrastText;
        text-decoration:underline;
    }
}

.faded_link, .faded_link:visited, .faded_link:link{
    color:$lightText;
    .faded_link:hover{
        color:$lightText;
        text-decoration:underline;
    }
}

.highlight_link, .highlight_link:link, .highlight_link:visited{
    color:$highlight;
    cursor:pointer;
}
.highlight_link:hover{
    text-decoration:underline;
}

.page_end{
    padding-top:20px;
    margin-top:10px;
    text-align:center;
}
/* LOADING ICONS (XLOADING) */

#loading_icon_container, .loading_icon_container{
    display:none;
    position:relative;
}
.loading_icon_container.show{
    display:block;
}

.loading_icon_left{
    content: url(../../images/loading.gif);
    height:30px;
    width:30px;
    position:absolute;
    left:0px;
}

.loading_icon_center{
    background-image: url(../../images/loading.gif);
    height:50px;
    width:50px;
}

#footer_panel{
    position:absolute;
    bottom:20px;
    right:20px;
}

#elevator{
    height:0px;
    width:100%;
}

#footer_container{
    margin-top:10px;
    width:500px;
    font-family:Arial;
    padding:10px;
    margin-left:auto;
    margin-right:auto;
    font-size:$fontDefaultSize;
    z-index:100;
    opacity:0.6;
    right:20px;
    text-shadow:0px 0px 2px $canvas;
    filter: alpha(opacity = 80);
    a{
        margin-left:20px;
    }
    > div{
        margin-left:20px;
    }
}

/* Wrapper Styles (XWRAPPER) */

#content_canvas{
    width: $feedDisplayWidth;
    margin-left:30px;
    z-index: 300;
    min-height:1010px;
    font-family:Arial;
    font-size:$fontDefaultSize;
    position:relative;
}

.main_canvas{
    font-family:Arial;
    padding:10px 20px 10px 20px;
    width:$maxContentWidth;
    .canvas_top{
        position:relative;
        .rightify{
            top:5px;
        }
    }
    .canvas_wide{
        margin-left:-20px;
        margin-right:-20px;
    }
    .canvas_content.padded{
        padding:20px;
    }
    .canvas_content.sidebar{
        display:inline-block;
        width:74%;
        vertical-align:top;
    }
    .content_sidebar{
        border-top:2px solid $border;
        width:170px;
        margin-top:10px;
        margin-bottom:20px;
        margin-right:20px;
        display:inline-block;
        vertical-align:top;
        .sidebar_item{
            padding:10px;
            border-bottom:1px solid lighten($imgBorder,45%);
            border-left:1px solid lighten($imgBorder,45%);
            border-right:1px solid lighten($imgBorder,35%);
            color:lighten($text,30%);
        }
        .sidebar_item:hover, .sidebar_item.current{
            color:$highlight;
        }
    }
}

.main_canvas.bg{
    border-radius:5px;
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
    -webkit-box-shadow: 0px 0px 5px $border;
    -moz-box-shadow: 0px 0px 5px $border;
    box-shadow: 0px 0px 5px $border;
    background:$canvas;
}

.main_canvas.empty{
    padding:0px;
    width:$maxContentWidth + 40;
}

.canvas_tile{
    padding:15px;
    border:1px solid lighten($imgBorder,8%);
    -webkit-box-shadow: 0px 0px 4px lighten($imgBorder,5%);
    -moz-box-shadow: 0px 0px 4px lighten($imgBorder,5%);
    box-shadow: 0px 0px 4px lighten($imgBorder,5%);
    border-radius:5px;
    -moz-border-radius:5px;
    -webkit-border-radius:5px;
    background-color:$canvas;
    margin-bottom:20px;
}


#user_top_menu{
    font-family:arial;
    margin-top:10px;
    font-size:$fontDefaultSize;
    position:relative;
    a{
        text-transform:uppercase;
        margin-right:20px;
    }
    .leftify{
        top:7px;
        left:15px;
    }
    .rightify{
        top:7px;
        right:15px;
    }    
}

.large_title{
    padding-left:11px;
    font-size:$fontXxlgSize;
    font-family:serif;
    font-style:italic;
    margin-top:10px;
    margin-bottom:15px;
}
.dialogue_panel_title{
    font-family:serif;
    font-style:italic;
    font-size:$fontXlgSize;

}
#content_canvas.wide{
    width: $maxContentWidth;
}

#content_canvas.special{
    width: $maxContentWidth - 250;
    margin-left:125px;
}

.canvas_align{
    width:$contentMaxWidth;
    margin-left:20px;
}

#content_body{
    width: $maxContentWidth;
    margin-left: auto;
    margin-right: auto;
    margin-top: $toolbarHeight + 30;
    position:relative;
}

body{
    background-color: $bodyBg;
    min-height:800px;
    font-family:Arial;
}

#background{
    top:0px;
    left:0px;
    width:100%;
    height:100%;
    display:block;
    position:fixed;
    z-index:-10;
    background-repeat:no-repeat;
}

#top_gutter{
    font-family:serif;
    position:fixed;
    z-index:800;
    width: 100%;
    height: 40px;
    padding-top:20px;
    font-style:italic;
    top: $toolbarHeight;
    left: 0px;
    /*background-color: $mainButtonColor;*/
    background-color: $offWhite;
    color:$text;
    border-bottom:3px solid $toolbarLightColor;
    display:none;
    > div{
        width: 1080px;
        margin-left: auto;
        margin-right: auto;
        margin-bottom:20px;
    }
}

#toolbar{
    display: block;
    position: fixed;
    z-index:800;
    width: 100%;
    height: $toolbarHeight;
    top: 0px;
    left: 0px;
    background-color: $toolbarBg;
    -webkit-box-shadow: -10px 0px 10px $shadow;
    -moz-box-shadow: -10px 0px 10px $shadow;
    box-shadow: -10px 0px 10px $shadow;
    > div{
        /*width: $maxContentWidth;*/
        margin-left: 10px;
        margin-right: 10px;
    }
    *{
        letter-spacing: 1px;
        font-family:Arial;
        font-size: $fontDefaultSize;
    }
    >div>.logo{
        position: absolute;
        background: none;
        opacity: 1;
        height:33px;
        width:140px;
        color:$text;
        z-index: 200;
        margin-top:14px;
        span{
            position:absolute;
            bottom:8px;
            left:65px;
            font-size:$fontNanoSize;
        }
    }
    .navigation_dropdown{
        background-color:$darkBg;
        position:absolute;
        z-index:700;
        width:190px;
        -webkit-box-shadow: 5px 5px 5px 0px rgba(red($shadow),green($shadow),blue($shadow),0.7);
        -moz-box-shadow: 5px 5px 5px 0px rgba(red($shadow),green($shadow),blue($shadow),0.7);
        box-shadow: 5px 5px 5px 0px rgba(red($shadow),green($shadow),blue($shadow),0.7);
        top:$toolbarHeight;
        >a, a:link, a:visited{
            color:$contrastText;
        }
        >a:hover{
            background-color: $toolbarBg;
        }
        >a >div{
            padding:10px;
        }
        >a >div:hover{
            background-color: $toolbarBg;
        }
    }
    .navigation_dropdown.account{
        right:187px;
    }
    .notification_dropdown{
        opacity: 0;
        -webkit-box-shadow: 0px 5px 5px 0px rgba(red($shadow),green($shadow),blue($shadow),0.7);
        -moz-box-shadow: 0px 5px 5px 0px rgba(red($shadow),green($shadow),blue($shadow),0.7);
        box-shadow: 0px 5px 5px 0px rgba(red($shadow),green($shadow),blue($shadow),0.7);
        visibility: hidden;
        display:block;
        width:345px;
        position:absolute;
        right:140px;
        background:$bright;
        letter-spacing:0;
        .notification_header{
            padding:10px 0px 10px 10px;
            background:#49494B;
            background-color:$darkBg;
            color:$contrastText;
        }
        .notification_footer{
            background-color:$canvasBg;
            color:$text;
            text-decoration:underline;
            padding:10px 0px 10px 10px;
            border-top:1px solid $lightBorder;
        }
        .notification_footer:hover{
            color:$highlight;
        }
        .dropdown_notification{
            background-color:$canvasBg;
            min-height:44px;
            padding:5px;
            .notify_message{
                letter-spacing:0;
                font-weight:bold;
                text-transform:none;
            }
            .content{
                float:left;
                width: 290px;
                margin-left:40px;
                font-size:12px;
                text-transform:none;
                letter-spacing:0;
            }
        }
        .dropdown_notification:hover{
            background-color:$lightBg;
        }
        .dropdown_notification.none{
            min-height:20px;
            padding-top:5px;
        }
    }
    #toolbar_navigation_inner{
        position:relative;
        margin-left:200px;
        padding-top:10px;
        padding-bottom:10px;
        height:$toolbarHeight - 20;
        overflow:hidden;
    }
    .nav_right{
        position:absolute;
        top:10px;
        right:150px;
        .nav_link{
            img{
                border:1px solid $lightShadow;
                width:28px;
                height:28px;
                position:absolute;
                top:12px;
                left:4px;
            }
        }
    }
    #toolbar_navigation{
        position:relative;
        z-index:200;
        left:150px;
        a.nav_link{
            font-size: $fontSmSize;
            position:relative;
            color:$toolbarLightColor;
            top:3px;
            padding:{
                left:10px;
                right:10px;
                top:20px;
                bottom:30px;
            }
        }
        a.nav_link:hover{
            text-decoration:none;
            color:$contrastText;
            background-color:$darkBg;
        }
        .toolbar_button_selected{
            background-color:$darkBg;
            text-decoration:none;
        }
        a.nav_link.first{
            margin-left:200px;
        }
        a.nav_link.break{
            margin-right:50px;
        }        

        .hover_tip_box{
            position:absolute;
            color:$text;
            z-index:700;
            margin-top:-28px;
            margin-left:320px;
            >.hover_tip{
                -moz-border-radius: 3px;
                border-radius: 3px;
                background-color:$shadow;

                text-transform:none;
                height:10px;
                position:relative;
                padding:{
                    bottom:8px;
                    top:8px;
                    right:10px;
                    left:10px;
                }
            }
            >.hover_tip:after{
                position:absolute;
                width:15px;
                height:15px;
                color:$shadow;
                right:-10px;
                margin-top:-3px;
                content:'\25B6';
                font-size:$fontLgSize;
            }
        }
        >#search{
            position:absolute;
            height:25px;
            width:192px;
            text-transform:none;
            overflow:hidden;
            -moz-border-radius: 5px;
            border-radius: 5px;
            -webkit-box-shadow: inset 2px 2px 5px $shadow;
            -moz-box-shadow: inset 2px 2px 5px $shadow;
            box-shadow: inset 2px 2px 5px $shadow;
            background-color:$lightBg;
            border:0;
            outline:0;
            margin-top:7px;
        }
        >#search input{
            height:20px;
            width:160px;
            text-transform:none;
            color:$text;
            font-size:$fontSmSize;
            -moz-border-radius: 5px;
            border-radius: 5px;
            border:1px inset $darkPanelHighlight;
            background: -moz-linear-gradient(top, $lightShadow 0%, $lightShadow 63%, $darkBg 100%); /* firefox */
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,$lightShadow), color-stop(63%,$darkBg)); 
            background-color:transparent;
            background-image:url(../../images/icons_small-v8.png);
            background-repeat:no-repeat;
            background-position:5px -445px;
            border:0;
            outline:0;
            padding:{
                top:3px;
                right:5px;
                bottom:3px;
                left:25px;
            }
        }
        /*
        >#search input:focus{
            outline:0;
            background-color:$toolbarLightColor;
            color:$text;
        }
        */
    }
    .searchRes{
        left:1100px;
        margin-top: 60px;
        background-color:red;
    }
    ul {
      padding: 0;
      margin: 0;
      list-style: none; }

    li {
      float: left;
      position: relative;
      margin :{
        right: 20px;
      }
      ul {
        display: none;
        position: absolute;
        top: 1em;
        left: 0; }
      > ul {
        top: auto;
        left: auto; }
      &:hover ul {
        display: block; } }
}

#growl_notification{
    background-color:$darkBg;
    height:30px;
    padding-left:20px;
    padding-right:20px;
    width:25%;
    z-index:500;  
    box-shadow: 0 0 10px black;
    position:fixed;
    border-radius:0px 0px 5px 5px;
    -moz-border-radius:0px 0px 5px 5px;
    font-family: 'arial';
    font-weight: normal;
    font-size: $fontSmSize;
    top:0px;
    left:62%;
    opacity: .8;
    border-bottom:1px solid #585153;
    >div.message{
        text-align:center;
        color:$contrastText;
        margin:13px 0px 0px 0px;
    }
}

#top_button{
    position:fixed;
    bottom:10px;
    margin-left:50px;
}


.notify_box{
    display:block;
    min-width:18px;
    height:22px;
    color:$contrastText;
    background:$highlight;
    border-radius:6px;
    -moz-border-radius:6px;
    -webkit-border-radius:6px;
    float:right;
    margin-top:-5px;
    padding-top:7px;
    padding-left:5px;
    padding-right:3px;
    margin-left:10px;
    z-index:500;
    text-align:center;
}

.notify_box.inactive{
    background:$lightShadow;
    border:1px solid $lightShadow;
}

/* SEARCH (XSEARCH) */

#messages_list{
    background-color:$canvas;
    font-family:Arial;
    position:fixed;
    top:$toolbarHeight - 10px;
    max-height: 400px;
    overflow:auto;
    z-index:100000;
    width:300px;
    -webkit-box-shadow: 1px 2px 4px $shadow;
    -moz-box-shadow: 1px 2px 4px $shadow;
    box-shadow: 1px 2px 4px $shadow;
    .search_title{
        padding:{
            top:5px;
            bottom:5px;
            left:10px;
            right:10px;
        }
        border-bottom:1px solid $lightShadow;
        font-size:$fontLgSize;
        height:20px;
        color:$contrastText;
        text-transform:none;
        background-color:$shadow;
    }
    .search_row{
        position:relative;
        height:40px;
        cursor:pointer;
        padding:{
            top:5px;
            bottom:5px;
            left:10px;
            right:10px;
        }
        >.search_name{
            text-transform:none;
        }
        >.search_subtext{
            font-size:$fontSmSize;
            color:$text;
            margin-top:8px;
            text-transform:none;
        }
        div >img{
            float:left;
            margin-right:10px;
            height:35px;
            width:35px;
            overflow:hidden;
        }
    }
     .search_title.link:hover{
        color:$highlight;
        background-color:$lightHighlight;
     }
}


/* DYNAMIC BUTTONS (XBUTTONS) */

.modal_tab_display{
    font-size:$fontDefaultSize;
    position:relative;
    font-family:Arial;
    min-height:18px;
    margin-top:-10px;
    padding:{
        top:12px;
        left:10px;
        right:10px;
        bottom:2px;
    }
    .modal_tab, .modal_tab:link, .modal_tab:visited{
        margin-right:1px;
        color:$highlight;
        padding:{
            top:5px;
            bottom:10px;
            left:7px;
            right:7px;
        }
    }
    .modal_tab.current{
        text-decoration:none;
        color:$text;
        position:relative;
        .current_tab_arrow{
            font-size:14px;
            position:absolute;
            bottom:0px;
            left:42%;
            color:$text;
        }
    }
}

.close_button{
    cursor:pointer;
    width:60px;
    height:12px;
    text-align:center;
    font-family: Verdana, Arial;
    color:$lightText;
    font-family:Arial;
    line-height:10px;
    span.close_text{
        font-family:Arial;
    }
}
.close_button.auto{
    width:auto;
    text-align:right;
}
.close_button:hover{
    color:$contrastText;
}

.dialogue_panel .close_button:hover{
    color:black;
}

.basic_button{
   -moz-border-radius: 5px;
   width:auto;
   font-style:normal;
   border-radius: 5px;
   border:1px solid $mainButtonBorder;
   padding:5px 16px 5px 16px;
   font-size:$fontSmSize;
   height:25px;
   margin:15px 0px 10px 2px;
}

.callout_button, .action_button, input[type=button], input[type=submit]{
   -moz-border-radius: 5px;
   width:auto;
   font-style:normal;
   border-radius: 5px;
   background:$mainButtonColor;
   border:1px solid $mainButtonBorder;
