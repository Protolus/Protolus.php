/* OVERRIDES */

a:link{ color:$text; text-decoration: none; }
a:visited{ color:$text; text-decoration: none; }
a:hover{ color:$highlight; text-decoration: underline; }
a:active{ color:$text; text-decoration: none; }

textarea{ resize: none; font-size:$fontLgSize; }

strong{ font-weight:bold; }
small{ font-size:$fontMicroSize; }

/* SEMANTIC */

.clear{ clear: both; }

.invisible{
    opacity:0;
    filter: alpha(opacity = 0);
}

.name{ color: $highlight; }
.light_text_color{ color:$lightText; }
.faded_text{ color:$lightText; }

.light_bg{
    background-color:$offWhite;
}

.facebook_icon{
   height:20px;
   margin-right:3px;
   width:20px;
   border:0;
   cursor:pointer;
